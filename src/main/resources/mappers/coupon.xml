<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.starbucksorder.another_back.repository.CouponMapper">


    <update id="updateUseDateById">
        UPDATE
            coupon_tb
        SET
            use_date = NOW();
        WHERE
            coupon_id = #{couponId}
    </update>

</mapper>
