<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.starbucksorder.another_back.repository.OrderDetailMapper">

    <insert id="save">
        insert into
            order_detail_tb
        values
        <foreach collection="orderDetails" item="orderDetail"></foreach>
        (default, #{orderId}, #{menuId}, #{quantity}, #{orderComment})
    </insert>
</mapper>